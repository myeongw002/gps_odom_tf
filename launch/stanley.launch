<launch>
  
  <arg name="path" default="$(find gps_odom_tf)/path/highway_path5.txt"/>
  
  <!--Nodes-->  
  <node pkg="gps_odom_tf" type="stanley2.py" name="stanley_control_node" output="screen"  >
    <param name="path" value="$(arg path)"/>
    <param name="K" value="0.7"/>
    <param name="L" value="1.04"/>
    <param name="max_steering" value="28.5"/>
    <param name="max_speed" value="15"/>
  </node> 
  
  <node pkg="gps_odom_tf" type="path_viz.py" name="path_visualize_node" output="screen">
    <param name="file_path" value="$(arg path)"/>
    <param name="R" value="0.5"/>
  </node>
   
   <node pkg="gps_odom_tf" type="gps_transformer.py" name="gps_transformer_node" output="screen">
     <param name="transform_params_path" value="$(find gps_odom_tf)/tf_data/transform_data2.txt"/>
   </node>
  

   
     
</launch>
